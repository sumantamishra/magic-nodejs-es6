{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "camelcase": ["error", { "properties": "always" }],
        "no-unused-vars": "error",
        "no-undefined": "error",
        "block-scoped-var": "error",
        "no-duplicate-imports": "error",
        "indent": ["error", 2]
    }
}